{
  "name": "nest_proj",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "yarn docker:up && yarn db:migrate:dep && yarn db:generate && nest start --watch",
    "docker:up": "docker compose up -d",
    "db:migrate:dev": "yarn prisma migrate dev --name init",
    "db:migrate:dep": "yarn prisma migrate deploy",
    "db:generate": "yarn prisma generate",
    "db:studio": "yarn prisma studio",
    "console": "ts-node src/console.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"{src,libs,test}/**/*.ts\" --fix",
    "prettier": "prettier --write \"src/**/*.{js,ts}\"",
    "format": "yarn type-check && yarn lint && yarn prettier",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "build": "nest build",
    "postbuild": "yarn db:migrate:dep",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main"
  },
  "dependencies": {
    "@apollo/server": "^4.10.5",
    "@as-integrations/fastify": "^2.1.1",
    "@fastify/cookie": "^9.3.1",
    "@nestjs/apollo": "^12.2.0",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-fastify": "^10.3.10",
    "@nestjs/schedule": "^4.1.0",
    "@prisma/client": "5.17.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-yet": "^5.1.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "google-auth-library": "^9.11.0",
    "graphql": "^16.9.0",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "moment": "^2.30.1",
    "nest-commander": "^3.14.0",
    "puppeteer": "^22.14.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "stripe": "^16.8.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@nestjs/cli": "^10.4.2",
    "@nestjs/schematics": "^10.1.3",
    "@nestjs/testing": "^10.3.10",
    "@types/bcrypt": "^5.0.2",
    "@types/cache-manager-redis-store": "^2.0.4",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/md5": "^2.3.5",
    "@types/node": "^20.14.12",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^5.17.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
